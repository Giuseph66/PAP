{
  "expo": {
    "name": "P A P ",
    "slug": "p-a-p",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/base.png",
    "scheme": "pap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.giuseph.pap",
      "config": {
        "googleMapsApiKey": "AIzaSyApkh47cDCfqWyGZ2izRglGwbzSx849S70"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a1a2e",
        "foregroundImage": "./assets/icons/base-sf.png",
        "backgroundImage": "./assets/icons/base.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.giuseph.pap",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyApkh47cDCfqWyGZ2izRglGwbzSx849S70"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/base-sf.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/tela-toda.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a2e",
          "dark": {
            "backgroundColor": "#1a1a2e"
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f4f21311-ca13-46db-a046-86cabff8669f"
      }
    }
  }
}
